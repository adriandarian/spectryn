{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "vitepress",
  "outputDirectory": ".vitepress/dist",
  "env": {
    "VERCEL_TOOLBAR_ENABLED": "0"
  },
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
